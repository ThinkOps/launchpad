# Top-level env file for docker compose (copy to .env and edit as needed)

# MySQL
MYSQL_ROOT_PASSWORD=root
MYSQL_DATABASE=launchpad
MYSQL_USER=launchpad
MYSQL_PASSWORD=launchpad

# Backend DB connectivity
DB_MASTER_HOST=lp-mysql
DB_READER_HOST=lp-mysql
DB_PORT=3306
DB_USERNAME=launchpad
DB_PASSWORD=launchpad
DB_NAME=launchpad
DB_LOGGING=false

# Auth
JWT_SECRET=change-me
GOOGLE_CLIENT_ID=change-me

# Frontend build/runtime
REACT_APP_ENV=development

# Reverse proxy (Caddy)
CADDY_DOMAIN=localhost
CADDY_EMAIL=admin@example.com

